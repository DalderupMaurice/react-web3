{
  "env": {
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "prettier"
  ],
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "react/destructuring-assignment": ["off"],
    "linebreak-style": ["off"],
    "prettier/prettier": "error",
    "react/forbid-prop-types": [1, { "forbid": ["any"] }]
  }
}
